<h1>Compare Videos{learner}</h1>

<div id="compareContainer">
	<div id="leftvid" class="vidContainer">{form1}</div>
	<div id="rightvid" class="vidContainer">{form2}</div>
	<!--NONARCHIVE-->
	<p style="text-align:center; clear:both">
		<form onsubmit="return false">
			<input type="hidden" id="learnerID" name="learnerID" value="{learnerID}">
			<textarea rows="10" cols="50" placeholder="Enter notes here..." id='review-notes'></textarea><br />
			<button id='comparison-add'>Submit Comparison</button>
		</form>
	</p>

	<p style="text-align:center; clear:both">
		<input type="button" value="Download as Archive" onclick="window.location=window.location.toString()+'/archive'" />
	</p>
	<!--/NONARCHIVE-->	
</div>